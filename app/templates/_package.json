    "babel-core": "^6.0.0",
    "babel-eslint": "^4.1.6",
    "babel-loader": "^6.0.0",
    "babel-polyfill": "^6.2.0",
    "babel-preset-es2015": "^6.0.15",
    "babel-eslint": "^4.1.4",
    "chai": "^3.2.0",
    "eslint": "^1.4.1",
    "eslint-plugin-babel": "^2.1.1",
    "husky": "^0.10.1",
    "isparta": "^4.0.0",
    "mocha": "^2.3.3",
    "rimraf": "^2.4.3",
    "webpack": "^1.12.2"
  },
  "author": {
    "name": "<%= name %>",
